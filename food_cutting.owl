<http://purl.obolibrary.org/obo/FOODON_00003415> a owl:Class ;
    rdfs:label "cucumber ",
        "cucumber (whole)"@en ;
    rdfs:comment "A pepo fruit of a cucumber plant (Cucumis sativus)." ;
    rdfs:subClassOf [ a owl:Class ;
            owl:intersectionOf ( [ a owl:Restriction ;
                        owl:onProperty :hasPart ;
                        owl:someValuesFrom :CucumberFlesh ] [ a owl:Restriction ;
                        owl:onProperty :hasPart ;
                        owl:someValuesFrom :CucumberSkin ] [ a owl:Restriction ;
                        owl:onProperty :hasPart ;
                        owl:someValuesFrom :CucumberStem ] ) ],
        [ a owl:Restriction ;
            owl:onProperty <http://www.ease-crc.org/ont/SOMA.owl#hasDisposition> ;
            owl:someValuesFrom [ a owl:Class ;
                    owl:intersectionOf ( :Peelability [ a owl:Restriction ;
                                owl:onProperty <http://www.ease-crc.org/ont/SOMA.owl#affordsTask> ;
                                owl:someValuesFrom :Peeling ] [ a owl:Restriction ;
                                owl:allValuesFrom [ a owl:Restriction ;
                                        owl:allValuesFrom :Peeler ;
                                        owl:onProperty <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#classifies> ] ;
                                owl:onProperty <http://www.ease-crc.org/ont/SOMA.owl#affordsTrigger> ] ) ] ],
        [ a owl:Restriction ;
            owl:hasValue :Green ;
            owl:onProperty :hasColor ],
        [ a owl:Restriction ;
            owl:hasValue "Cucumis" ;
            owl:onProperty :genus ],
        [ a owl:Restriction ;
            owl:onProperty :hasPart ;
            owl:someValuesFrom [ a owl:Class ;
                    owl:intersectionOf ( :Stem [ a owl:Restriction ;
                                owl:onProperty :hasEdibility ;
                                owl:someValuesFrom :ShouldBeAvoided ] ) ] ],
        [ a owl:Restriction ;
            owl:onProperty <http://www.ease-crc.org/ont/SOMA.owl#hasDisposition> ;
            owl:someValuesFrom [ a owl:Class ;
                    owl:intersectionOf ( :StemRemovability [ a owl:Restriction ;
                                owl:onProperty <http://www.ease-crc.org/ont/SOMA.owl#affordsTask> ;
                                owl:someValuesFrom :StemRemoving ] [ a owl:Restriction ;
                                owl:allValuesFrom [ a owl:Restriction ;
                                        owl:allValuesFrom <http://www.ease-crc.org/ont/SOMA.owl#CuttingTool> ;
                                        owl:onProperty <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#classifies> ] ;
                                owl:onProperty <http://www.ease-crc.org/ont/SOMA.owl#affordsTrigger> ] ) ] ],
        [ a owl:Restriction ;
            owl:hasValue :Medium ;
            owl:onProperty :hasSize ],
        [ a owl:Restriction ;
            owl:onProperty <http://www.ease-crc.org/ont/SOMA.owl#hasDisposition> ;
            owl:someValuesFrom [ a owl:Class ;
                    owl:intersectionOf ( <http://www.ease-crc.org/ont/situation_awareness#Cuttability> [ a owl:Restriction ;
                                owl:onProperty <http://www.ease-crc.org/ont/SOMA.owl#affordsTask> ;
                                owl:someValuesFrom <http://www.ease-crc.org/ont/SOMA.owl#Cutting> ] [ a owl:Restriction ;
                                owl:allValuesFrom [ a owl:Restriction ;
                                        owl:allValuesFrom <http://www.ease-crc.org/ont/SOMA.owl#CuttingTool> ;
                                        owl:onProperty <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#classifies> ] ;
                                owl:onProperty <http://www.ease-crc.org/ont/SOMA.owl#affordsTrigger> ] ) ] ],
        [ a owl:Restriction ;
            owl:allValuesFrom :Oval ;
            owl:onProperty :hasInitialShape ],
        <http://purl.obolibrary.org/obo/PO_0009001>,
        :vegetable .

